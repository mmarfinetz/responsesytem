# Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_API_KEY=dev-api-key-for-frontend-backend-communication

# Application Configuration
VITE_APP_NAME=Plumbing Voice AI
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true

# Google Voice Configuration (for webhook URLs)
VITE_GOOGLE_VOICE_WEBHOOK_URL=http://localhost:3001/webhooks/google-voice

# WebSocket Configuration (for real-time updates)
VITE_WS_URL=ws://localhost:3001

# File Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/*,audio/*,video/*

# Map Integration (for technician routing)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Development Settings
VITE_DEV_MODE=true
VITE_LOG_LEVEL=info